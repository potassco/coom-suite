product {
	Grundmerkmale	grundmerkmale
	Konstruktion	konstruktion
	Cover			cover
}

structure Grundmerkmale {
	num/mm		montagehoehe
	num/mm		durchgangshoehe
	num/mm		laenge
	num.#/mm	eingabeBreite
	num.#/mm	bestellBreite
	num.##/qm	flaeche
	num.##/Grad	neigungswinkelAnlage
	num.##/Kg	weight
	// Debug		debugValues
}

