product{
    1x   Element     elements
    2x   Module modules
}

structure Module {
    ModuleType type
}

enumeration ModuleType {
    I
    II
}


structure Element {
    reference   Module      modules
                ElementType type
}

enumeration ElementType {
    A
    // B
}

behavior Element {
    combinations ( type modules.type )
    allow        (   A      I        )
    // allow        (   B      II        )
}
